@using Agenda.MVC.ViewModel
@model EditUserViewModel

@{
    ViewData["Title"] = "Alterar user";
}

<div>
    @using(Html.BeginForm("Edit", "UserAdmin", FormMethod.Post))
    {
        <div>
          @Html.LabelFor(x => x.UserRoleId)
          @Html.TextBoxFor(x => x.UserRoleId)
          @Html.ValidationMessageFor(x => x.UserRoleId)
        </div>
        @await Html.PartialAsync("_FormsEditUser", Model)
    }

</div>
@section Scripts{
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
